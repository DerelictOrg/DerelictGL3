name "derelict-gl3"
description "A dynamic binding to the OpenGL library."
homepage "https://github.com/DerelictOrg/DerelictGL3"
authors "Mike Parker"
license "Boost"
targetPath "lib"
targetName "DerelictGL3"
dependency "derelict-util" version="3.0.0"

configuration "default" {
    versions "DerelictGL3_NoContexts"
}

configuration "use-contexts" {
    versions "DerelictGL3_UseContexts"
}